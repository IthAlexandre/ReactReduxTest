{
  "name": "reactredux",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.24.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "moxios": "^0.4.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "^5.1.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^5.0.0",
    "redux": "^4.1.2",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.4.1",
    "tv4": "^1.3.0"
